{
    "author": "Sebastien Rousseau <sebastienrousseau@users.noreply.github.com>",
    "autoupdate": {
        "fileMap": [
            {
                "basePath": "dist",
                "files": [
                    "**/*"
                ]
            }
        ],
        "source": "git",
        "target": "git://github.com/sebastienrousseau/pm2md.git"
    },
    "bin": {
        "pm2md": "./dist/bin/global.js"
    },
    "bugs": {
        "url": "https://github.com/sebastienrousseau/pm2md/issues"
    },
    "description": "Generates a markdown documentation from a Postman collection file.",
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@sebastienrousseau/eslint-config": "^0.0.2",
        "@sebastienrousseau/jsdoc-config": "^0.0.5",
        "@sebastienrousseau/markdownlint-config": "^0.0.1",
        "@sebastienrousseau/prettier-config": "^0.0.4",
        "@sebastienrousseau/remark-config": "^0.0.3",
        "@types/jest": "^28.1.6",
        "@types/minimist": "^1.2.2",
        "@types/node": "^18.6.1",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "chalk": "^4.1.2",
        "eslint": "^8.20.0",
        "eslint-import-resolver-typescript": "^3.3.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "filesizes": "^0.1.2",
        "husky": "^8.0.1",
        "jest": "^28.1.3",
        "minimist": "^1.2.6",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "remark-cli": "^11.0.0",
        "remark-footnotes": "^4.0.1",
        "remark-preset-lint-consistent": "^5.1.1",
        "remark-preset-lint-markdown-style-guide": "^5.1.2",
        "remark-preset-lint-recommended": "^6.1.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.9",
        "typedoc-plugin-missing-exports": "^0.23.0",
        "typescript": "^4.7.4"
    },
    "directories": {
        "src": "src",
        "test": "__tests__"
    },
    "engines": {
      "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
      "dist/**/*"
  ],
  "funding": [
      {
          "type": "github",
          "url": "https://github.com/sponsors/sebastienrousseau"
      },
      {
          "type": "paypal",
          "url": "https://paypal.me/wwdseb"
      }
  ],
  "homepage": "https://pm2md.io",
  "keywords": [
      "crypto-cli"
  ],
  "license": "MIT",
    "license_URI": "http://www.opensource.org/licenses/mit-license.php",
    "main": "./dist/index.js",
    "name": "pm2md",
    "preferGlobal": true,
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sebastienrousseau/pm2md.git"
    },
    "scripts": {
        "compile": "npx tsc --build tsconfig.json",
        "compile:watch": "npx tsc -w",
        "clean": "npx rimraf dist coverage node_modules",
        "docs": "npx typedoc --options typedoc.json src/**.ts",
        "filesize:distribution": "npx filesizes dist/ > dist/report.txt",
        "format": "prettier --write src/**/*.ts",
        "lint": "npx eslint ./src/**/*.ts ./__tests__/**/*.ts",
        "lint:fix": "npx eslint --fix ./src/**/*.ts ./__tests__/**/*.ts",
        "markdown": "npx remark ./*.md packages/**/*.md --rc-path ./.remarkrc --quiet",
        "postinstall": "husky install",
        "prepack": "npx exec build",
        "release": "npx exec release:prepare && yarn release:publish",
        "release:prepare": "npx exec pack",
        "release:publish": "npx exec publish --access public --ignore-scripts --@OWNER:registry='https://registry.npmjs.org' && npm publish",
        "start": "node ./dist/src/bin/global.js",
        "test": "npx jest --coverage --config jest.config.ts",
        "test:watch": "npx jest --watch",
        "watch": "npx jest --watch --config jest.config.ts"
    },
    "typings": "dist/index.d.ts",
    "version": "1.0.7"
}
