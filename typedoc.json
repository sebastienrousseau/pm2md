{
    "$schema": "https://typedoc.org/schema.json",
    "cleanOutputDir": true,
    "entryPointStrategy": "expand",
    "entryPoints": [
        "."
    ],
    "exclude": [
        "src/!(@types)/*/*"
    ],
    "excludeExternals": true,
    "excludeInternal": true,
    "excludePrivate": true,
    "githubPages": false,
    "logLevel": "Verbose",
    "name": "pm2md",
    "out": "docs",
    "plugin": [
        "typedoc-plugin-missing-exports",
    ],
    "readme": "README.md",
    "sort": [
        "source-order"
    ]
}
